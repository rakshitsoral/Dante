    <body {% if body_class %}id="{{ body_class }}" {% endif %}class="flex flex-col h-screen bg-sitebg text-gray-100 text-base font-sans break-words">
        <div class="max-w-5xl w-full mx-auto px-2 flex-grow">
            {% include "partials/header.njk" %}
            <main>
                {% if layout === "post" %}
                {{ content | safe }}
                {% else %}
                {{ content | safe }}
                {% endif %}
            </main>
            {% include "partials/footer.njk" %}
        </div>
        <script src="{{ '/assets/js/bundle.js' | url }}"></script>
        {% if highlightjs %}
        <script src="//cdnjs.cloudflare.com/ajax/libs/highlight.js/10.7.2/highlight.min.js"></script>
        <script>hljs.initHighlightingOnLoad();</script>
        {% endif %}
        {% if roughjs %}
        <script src="https://unpkg.com/rough-notation/lib/rough-notation.iife.js"></script>
        <script>
        setTimeout(function () {
            const annotate = RoughNotation.annotate;
            const annotationGroup = RoughNotation.annotationGroup;
            const $ = (t) => document.querySelector(t);
            {
                const a1 = annotate($('.box-blue'), { type: 'box', color: '#3BC9DB', animationDuration: 500 });
                const a2 = annotate($('.box-green'), { type: 'box', color: '#059669', animationDuration: 500 });
                const a3 = annotate($('.box-red'), { type: 'box', color: '#B91C1C', animationDuration: 500 });
                const a4 = annotate($('.underline-gray'), { type: 'underline', color: '#475569', animationDuration: 500, iterations: 2 });
                const a5 = annotate($('.underline-gray2'), { type: 'underline', color: '#475569', animationDuration: 500, iterations: 2 });
                const ag = annotationGroup([a1, a2, a3, a4, a5]);
                ag.show();
            }
        }, 500)();
        </script>
        {% endif %}
